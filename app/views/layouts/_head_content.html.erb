<% title = 'ResiTown Sacramento' %>
<% subtitle = '' %>

<link rel="apple-touch-icon" sizes="180x180" href="<%= content_for(:dynamic_fav_ico) || asset_path('favicon/apple-touch-icon.png') %>"/>
<link rel="icon" type="image/png" sizes="32x32" href="<%= content_for(:dynamic_fav_ico) || asset_path('favicon/favicon-32x32.png') %>"/>
<link rel="icon" type="image/png" sizes="16x16" href="<%= content_for(:dynamic_fav_ico) || asset_path('favicon/favicon-16x16.png') %>"/>
<link rel="shortcut icon" href="<%= content_for(:dynamic_fav_ico) || asset_path('favicon/favicon.ico') %>"/>
<meta name="theme-color" content="#020f80"/>

<meta property="og:type" content="article"/>
<meta property="og:title" content="<%= content_for(:share_title) || title %>"/>
<meta property="og:description" content="<%= content_for(:share_desc) || subtitle %>"/>
<meta property="og:url" content="<%= content_for(:share_url) || "https://www.resitowncom" %>"/>
<meta property="og:site_name" content="<%= content_for(:share_title) || title %>"/>
<meta property="og:image" content="<%= content_for(:share_img) || "https://www.helpwithcovid.com/share-image.png" %>"/>
<meta property="og:image:width" content="1200"/>
<meta property="og:image:height" content="630"/>

<meta name="twitter:card" content="<%= content_for(:share_twitter_card_type) || "summary_large_image" %>"/>
<meta name="twitter:title" content="<%= content_for(:share_title) || title %>"/>
<meta name="twitter:description" content="<%= content_for(:share_desc) || subtitle %>"/>
<meta name="twitter:url" content="<%= content_for(:share_url) || "https://www.helpwithcovid.com" %>"/>
<meta name="twitter:image" content="<%= content_for(:share_img) || "https://www.helpwithcovid.com/share-image.png" %>"/>

<meta rel="canonical" content="<%= content_for(:canonical_url) || request.url %>"/>
